
Host *
	ServerAliveInterval 67
	TCPKeepAlive no

	ConnectTimeout 5
	ConnectionAttempts 3

	KbdInteractiveAuthentication no
	ChallengeResponseAuthentication no
	GSSAPIAuthentication no

	User user
	IdentitiesOnly yes

	UserKnownHostsFile ~/.ssh/known_hosts.d/default

	ForwardAgent no
	ForwardX11 no
